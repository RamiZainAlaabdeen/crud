<AnchorPane prefHeight="400.0" prefWidth="213.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.crud.LoginPageController">
   <children>
      <Label layoutX="143.0" layoutY="34.0" prefHeight="76.0" prefWidth="213.0" text="Login Page">
         <font>
            <Font size="43.0" />
         </font>
      </Label>
      <Label layoutX="95.0" layoutY="158.0" text="username">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label layoutX="96.0" layoutY="228.0" text="password">
